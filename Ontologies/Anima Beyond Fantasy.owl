    <!-- urn:absolute:project_arpg/ontologies/anima_beyond_fantasy#Mantenimiento -->

    <owl:NamedIndividual rdf:about="urn:absolute:project_arpg/ontologies/anima_beyond_fantasy#Mantenimiento">
