        <Arma_Tipología rdf:resource="urn:absolute:project_arpg/ontologies/anima_beyond_fantasy#Tipo_Arma_Corta"/>
        <Equipo_Disponibilidad rdf:resource="urn:absolute:project_arpg/ontologies/anima_beyond_fantasy#Equipo_Raro"/>
        <Arma_DañoBase rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">20</Arma_DañoBase>
        <Arma_Entereza rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">9</Arma_Entereza>
        <Arma_Fuerza_1Mano rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</Arma_Fuerza_1Mano>
        <Arma_Presencia rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">25</Arma_Presencia>
        <Arma_Rotura rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">-2</Arma_Rotura>
        <Arma_Turno rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">10</Arma_Turno>
        <Equipo_Coste rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3000</Equipo_Coste>
        <Equipo_Peso rdf:datatype="http://www.w3.org/2001/XMLSchema#float">0.3</Equipo_Peso>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Artilugio en forma de garras, que se lleva en la palma de las manos. Se utiliza como arma y como apoyo para trepar, habilidad en la que otorga un bono de +10.</rdfs:comment>
    </owl:NamedIndividual>
    


