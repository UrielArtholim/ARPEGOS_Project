    <!-- urn:absolute:project_arpg/ontologies/anima_beyond_fantasy#Vasallaje -->

    <owl:NamedIndividual rdf:about="urn:absolute:project_arpg/ontologies/anima_beyond_fantasy#Vasallaje">
