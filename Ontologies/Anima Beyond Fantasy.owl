    <!-- urn:absolute:project_arpg/games/anima_beyond_fantasy#Personaje_Jugador -->

    <owl:NamedIndividual rdf:about="urn:absolute:project_arpg/games/anima_beyond_fantasy#Personaje_Jugador">
