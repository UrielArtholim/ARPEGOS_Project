        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Son ataques cortantes que causan heridas mediante tajos. Es el tipo que usan las armas afiladas.</rdfs:comment>
    </owl:NamedIndividual>
    


    <!-- urn:absolute:project_arpg/ontologies/anima_beyond_fantasy#Ataque_Frío -->

    <owl:NamedIndividual rdf:about="urn:absolute:project_arpg/ontologies/anima_beyond_fantasy#Ataque_Frío">
