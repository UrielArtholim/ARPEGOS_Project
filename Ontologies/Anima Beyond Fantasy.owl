    <!-- urn:absolute:project_arpg/ontologies/anima_beyond_fantasy#Arco_Compuesto -->

    <owl:NamedIndividual rdf:about="urn:absolute:project_arpg/ontologies/anima_beyond_fantasy#Arco_Compuesto">
        <rdf:type rdf:resource="urn:absolute:project_arpg/ontologies/anima_beyond_fantasy#Arcos"/>
        <Arma_Especial rdf:resource="urn:absolute:project_arpg/ontologies/anima_beyond_fantasy#A_dos_manos"/>
        <Arma_Rango rdf:resource="urn:absolute:project_arpg/ontologies/anima_beyond_fantasy#Arma_a_distancia"/>
        <Arma_Tipología rdf:resource="urn:absolute:project_arpg/ontologies/anima_beyond_fantasy#Tipo_Proyectil"/>
        <Equipo_Disponibilidad rdf:resource="urn:absolute:project_arpg/ontologies/anima_beyond_fantasy#Equipo_Raro"/>
        <Arma_Entereza rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</Arma_Entereza>
        <Arma_Fuerza_2Manos rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</Arma_Fuerza_2Manos>
        <Arma_Presencia rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">25</Arma_Presencia>
        <Arma_Rotura rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">-2</Arma_Rotura>
        <Arma_Turno rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">-3</Arma_Turno>
        <Equipo_Coste rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">80000</Equipo_Coste>
        <Equipo_Peso rdf:datatype="http://www.w3.org/2001/XMLSchema#float">1.8</Equipo_Peso>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Se trata de los arcos más grandes y potentes. Están formados por tres piezas y miden sobre los dos metros y medio de largo. Requieren ambas manos para usarlos, pero no doblan el bono de Fuerza.</rdfs:comment>
    </owl:NamedIndividual>
    


    <!-- urn:absolute:project_arpg/ontologies/anima_beyond_fantasy#Arco_Corto -->

    <owl:NamedIndividual rdf:about="urn:absolute:project_arpg/ontologies/anima_beyond_fantasy#Arco_Corto">
        <rdf:type rdf:resource="urn:absolute:project_arpg/ontologies/anima_beyond_fantasy#Arcos"/>
        <Arma_Especial rdf:resource="urn:absolute:project_arpg/ontologies/anima_beyond_fantasy#A_dos_manos"/>
        <Arma_Rango rdf:resource="urn:absolute:project_arpg/ontologies/anima_beyond_fantasy#Arma_a_distancia"/>
        <Arma_Tipología rdf:resource="urn:absolute:project_arpg/ontologies/anima_beyond_fantasy#Tipo_Proyectil"/>
        <Equipo_Disponibilidad rdf:resource="urn:absolute:project_arpg/ontologies/anima_beyond_fantasy#Equipo_Común"/>
        <Arma_Entereza rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</Arma_Entereza>
        <Arma_Fuerza_2Manos rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</Arma_Fuerza_2Manos>
        <Arma_Presencia rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">15</Arma_Presencia>
        <Arma_Rotura rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">-3</Arma_Rotura>
        <Arma_Turno rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">-10</Arma_Turno>
        <Equipo_Coste rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5000</Equipo_Coste>
        <Equipo_Peso rdf:datatype="http://www.w3.org/2001/XMLSchema#float">0.8</Equipo_Peso>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Compuesto por un único palo curvo de madera flexible, en cuyos extremos se une una cuerda tensada. Son arcos con un tamaño inferior a 120 centímetros de largo. Requiere ambas manos para usarlo, pero no dobla el bono de Fuerza.</rdfs:comment>
    </owl:NamedIndividual>
    


    <!-- urn:absolute:project_arpg/ontologies/anima_beyond_fantasy#Arco_Largo -->

    <owl:NamedIndividual rdf:about="urn:absolute:project_arpg/ontologies/anima_beyond_fantasy#Arco_Largo">
        <rdf:type rdf:resource="urn:absolute:project_arpg/ontologies/anima_beyond_fantasy#Arcos"/>
        <Arma_Especial rdf:resource="urn:absolute:project_arpg/ontologies/anima_beyond_fantasy#A_dos_manos"/>
        <Arma_Rango rdf:resource="urn:absolute:project_arpg/ontologies/anima_beyond_fantasy#Arma_a_distancia"/>
        <Arma_Tipología rdf:resource="urn:absolute:project_arpg/ontologies/anima_beyond_fantasy#Tipo_Proyectil"/>
        <Equipo_Disponibilidad rdf:resource="urn:absolute:project_arpg/ontologies/anima_beyond_fantasy#Equipo_Común"/>
        <Arma_Entereza rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</Arma_Entereza>
        <Arma_Fuerza_2Manos rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</Arma_Fuerza_2Manos>
        <Arma_Presencia rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">20</Arma_Presencia>
        <Arma_Rotura rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">-2</Arma_Rotura>
        <Arma_Turno rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">-30</Arma_Turno>
        <Equipo_Coste rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">20000</Equipo_Coste>
        <Equipo_Peso rdf:datatype="http://www.w3.org/2001/XMLSchema#float">1.4</Equipo_Peso>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Como el arco corto, pero con una dimensión comprendida entre el metro y medio y los dos metros. Requiere ambas manos para usarlo, pero no dobla el bono de Fuerza.</rdfs:comment>
    </owl:NamedIndividual>
    


    <!-- urn:absolute:project_arpg/ontologies/anima_beyond_fantasy#Arcón_Grande -->

    <owl:NamedIndividual rdf:about="urn:absolute:project_arpg/ontologies/anima_beyond_fantasy#Arcón_Grande">
        <rdf:type rdf:resource="urn:absolute:project_arpg/ontologies/anima_beyond_fantasy#Utiles_Varios"/>
    </owl:NamedIndividual>
    


    <!-- urn:absolute:project_arpg/ontologies/anima_beyond_fantasy#Arcón_Pequeño -->

    <owl:NamedIndividual rdf:about="urn:absolute:project_arpg/ontologies/anima_beyond_fantasy#Arcón_Pequeño">
        <rdf:type rdf:resource="urn:absolute:project_arpg/ontologies/anima_beyond_fantasy#Utiles_Varios"/>
    </owl:NamedIndividual>
    


    <!-- urn:absolute:project_arpg/ontologies/anima_beyond_fantasy#Arma -->

    <owl:NamedIndividual rdf:about="urn:absolute:project_arpg/ontologies/anima_beyond_fantasy#Arma">
        <rdf:type rdf:resource="urn:absolute:project_arpg/ontologies/anima_beyond_fantasy#Definicion_Clases"/>
    </owl:NamedIndividual>
    


    <!-- urn:absolute:project_arpg/ontologies/anima_beyond_fantasy#Arma_Cuerpo_a_Cuerpo -->

    <owl:NamedIndividual rdf:about="urn:absolute:project_arpg/ontologies/anima_beyond_fantasy#Arma_Cuerpo_a_Cuerpo">
