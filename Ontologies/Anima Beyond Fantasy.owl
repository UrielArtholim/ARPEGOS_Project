    <!-- urn:absolute:project_arpg/ontologies/anima_beyond_fantasy#Ver_en_las_Cenizas -->

    <owl:NamedIndividual rdf:about="urn:absolute:project_arpg/ontologies/anima_beyond_fantasy#Ver_en_las_Cenizas">
        <rdf:type rdf:resource="urn:absolute:project_arpg/ontologies/anima_beyond_fantasy#Libro_Fuego"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Nivel: 32 
