    <!-- urn:absolute:project_arpg/ontologies/anima_beyond_fantasy#Vuelo_Ki -->

    <owl:NamedIndividual rdf:about="urn:absolute:project_arpg/ontologies/anima_beyond_fantasy#Vuelo_Ki">
