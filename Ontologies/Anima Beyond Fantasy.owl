    <!-- urn:absolute:project_arpg/ontologies/anima_beyond_fantasy#Velocitas_Ventus -->

    <owl:NamedIndividual rdf:about="urn:absolute:project_arpg/ontologies/anima_beyond_fantasy#Velocitas_Ventus">
