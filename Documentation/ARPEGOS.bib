@incollection{Shaw1995,
abstract = {Software architecture is the level of software design that addresses$\backslash$nthe overall structure and properties of software systems. It provides$\backslash$na focus for certain aspects of design and development that are not$\backslash$nappropriately addressed within the constituent modules. Architectural$\backslash$ndesign depends heavily on accurate specifications of subsystems and$\backslash$ntheir interactions. These specifications must cover a wide variety$\backslash$nof properties, so the specification notations and associated methods$\backslash$nmust be selected or developed to match the properties of interest.$\backslash$nUnfortunately, the available formal methods are only a partial match$\backslash$nfor architectural needs, which entail description of structure, packaging,$\backslash$nenvironmental assumptions, representation, and performance as well$\backslash$nas functionality. A prerequisite for devising or selecting a formal$\backslash$nmethod is sound understanding of what needs to be formalized. For$\backslash$nsoftware architecture, much of this understanding is arising through$\backslash$nprogressive codification, which begins with real-world examples and$\backslash$nprogressively creates more precise models that eventually support$\backslash$nformalization. This paper explores the progressive codification of$\backslash$nsoftware architectures: the relation between emerging models and$\backslash$nthe selection, development, and use of formal systems.},
author = {Shaw, Mary and Garlan, David},
doi = {10.1007/bfb0015251},
file = {:D$\backslash$:/ARPEGOS{\_}Project/Documentation/Docs/Shaw, Garlan - 1995 - Formulations and formalisms in software architecture.pdf:pdf},
pages = {307--323},
title = {{Formulations and formalisms in software architecture}},
volume = {1000},
year = {1995}
}
@phdthesis{Tavora2014,
abstract = {The increased use of cosmetic products, in particular UV filter compounds, results with their introduction in significant amounts into aquatic ecosystems. Thus, the study and understanding of these substances in the environment is of major importance because of their potential to bioaccumulate and consequent toxicity, which may have a negative impact on the receiving environment, especially for biota, or even humans. Moreover, the contamination of these compounds is further aggravated in urban ecosystems, as fallout of population pressure and human activities. This master project seeks to analyze the impact of these substances in urban environments by having the city of Barcelona and its surroundings as a case study. To accomplish this, first an extensive literature review on characteristics, environmental and toxicity levels of UV filter compounds was performed. Secondly, water samples from rivers and WWTPs from the urban surroundings of Barcelona were analyzed so as to check the levels of mentioned substances, as well as to calculate the removal efficiency of the treatment stations and the potential risk posed by levels audited.},
author = {T{\'{a}}vora, JOAO MARTINS FERREIRA MIRANDA DE},
file = {:D$\backslash$:/ARPEGOS{\_}Project/Documentation/Docs/T{\'{a}}vora - 2014 - Tesis de m{\'{a}}ster.pdf:pdf},
pages = {88},
title = {{Tesis de m{\'{a}}ster}},
year = {2014}
}
