  <rdf:Description rdf:about="http://arpegos_project/Games/Anima_Beyond_Fantasy/characters/Guybrush_Threepwood#Personaje_Jugador">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class" />
