    <rdfs:subPropertyOf rdf:resource="http://arpegos_project/Games/Anima_Beyond_Fantasy/characters/Test#DesventajaTieneValor" />
    <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#unsignedInt">3</rdfs:isDefinedBy>
  </rdf:Description>
  <rdf:Description rdf:about="http://arpegos_project/Games/Anima_Beyond_Fantasy/characters/Test#Desventaja_Mística">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class" />
    <rdfs:subClassOf rdf:resource="http://arpegos_project/Games/Anima_Beyond_Fantasy/characters/Test#Desventaja" />
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Sólo quienes tengan el Don podrán elegir desventajas de esta lista. Los Puntos de Creación que consigas con ellas sólo podrán utilizarse para comprar ventajas mágicas para personajes con el Don. De todas formas, recuerda que el total de desventajas no puede ser mayor de tres, por lo que si ya has elegido tres generales, no podrás elegir ninguna de aquí.</rdfs:comment>
    <Test:SubstageOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</Test:SubstageOrder>
  </rdf:Description>
  <rdf:Description rdf:about="http://arpegos_project/Games/Anima_Beyond_Fantasy/characters/Test#Desventaja_Psíquica">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class" />
